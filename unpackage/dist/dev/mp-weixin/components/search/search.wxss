.content {
  margin-top: 50rpx;
  display: flex;
  align-items: center;
  z-index: 1;
  text-align: c;
  height: 60rpx;
  justify-content: space-between;
  border-top: 4rpx solid #ee6a02;
  border-left: 4rpx solid #f69b2b;
  border-bottom: 4rpx solid #ffc951;
  border-right: 4rpx solid #f7c895;
  box-shadow: 3px 2px #ee6a02;
  border-radius: 30rpx;
  padding: 2rpx 30rpx;
  width: 600rpx;
}
.content .iconfont {
  font-size: 40rpx;
}
.content .search-butten {
  height: 80%;
  background-color: #e50012;
  text-align: center;
  align-items: center;
  width: 120rpx;
  border-radius: 30rpx;
  color: #ffffff;
}

